@charset "utf-8";

.fuwu_wrap{margin:50px auto;background:#fff;box-shadow: 0 2px 4px #B0B0B0;border-radius:5px;overflow: hidden;}
.fuwu_wrap .line{width:100%;overflow:hidden;}
.fuwu_wrap .box{float:left;width:50%;}
.fuwu_wrap .box_ct{margin:30px;display:block;}
.fuwu_wrap .box_ct .icon{display:block;width:100px;height:100px;border-radius:100%;margin:0 auto;background:url(img/fuwu_icons.png) no-repeat #3398DB;}
.fuwu_wrap .box_ct .icon1{background-position:0 0;}
.fuwu_wrap .box_ct .icon2{background-position:0 -100px;}
.fuwu_wrap .box_ct .icon3{background-position:0 -200px;;}
.fuwu_wrap .box_ct .icon4{background-position:0 -300px;;}
.fuwu_wrap .box_ct .icon5{background-position:0 -400px;;}
.fuwu_wrap .box_ct .icon6{background-position:0 -500px;;}
.fuwu_wrap .box_ct b{display: block;line-height:20px;font-size:14px;color:#4A4A4A;font-weight: 600;text-align: center;padding:16px 0 10px;}
.fuwu_wrap .box_ct p{line-height:20px;color:#4A4A4A;font-size:12px;text-indent: 2em;}


